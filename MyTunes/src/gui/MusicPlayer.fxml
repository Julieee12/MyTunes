<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MPController">
   <children>
      <Button fx:id="prevsong" layoutX="326.0" layoutY="30.0" mnemonicParsing="false" onAction="#playPreviousSong" prefHeight="16.0" prefWidth="42.0" text="Prev" />
      <Button fx:id="playPause" alignment="CENTER" layoutX="395.0" layoutY="18.0" mnemonicParsing="false" onAction="#playSongPause" prefHeight="50.0" prefWidth="50.0">
         <graphic>
            <ImageView fx:id="playImage" fitHeight="42.0" fitWidth="39.0" onDragDetected="#play" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@play.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="nextsong" layoutX="475.0" layoutY="30.0" mnemonicParsing="false" onAction="#platNextSong" text="Next" />
      <Slider layoutX="209.0" layoutY="112.0" prefHeight="14.0" prefWidth="426.0" />
      <Label layoutX="25.0" layoutY="134.0" prefHeight="26.0" prefWidth="67.0" text="Playlists" />
      <Label fx:id="currentlyPlayingSong" layoutX="375.0" layoutY="82.0" prefHeight="17.0" prefWidth="93.0" text="...song is Playing" />
      <Label layoutX="543.0" layoutY="139.0" text="All Songs" />
      <Button fx:id="newsong" layoutX="543.0" layoutY="553.0" mnemonicParsing="false" onAction="#addNewSong" text="New" />
      <ListView fx:id="songsInPlaylist" layoutX="239.0" layoutY="168.0" prefHeight="376.0" prefWidth="183.0" />
      <TextField fx:id="searchInput" layoutX="692.0" layoutY="65.0" prefHeight="25.0" prefWidth="123.0" />
      <Text layoutX="652.0" layoutY="83.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Filter" />
      <Button fx:id="newPlaylist" layoutX="20.0" layoutY="553.0" mnemonicParsing="false" onAction="#addNewPlaylist" text="New" />
      <Button fx:id="deletePlaylistID" layoutX="170.0" layoutY="553.0" mnemonicParsing="false" onAction="#deletePlaylist" text="Delete" />
      <Button fx:id="deleteSongPlaylistID" layoutX="370.0" layoutY="553.0" mnemonicParsing="false" onAction="#deleteSongInPlaylist" text="Delete" />
      <Button fx:id="deleteSongsID" layoutX="739.0" layoutY="553.0" mnemonicParsing="false" onAction="#deleteSongs" text="Delete" />
      <Button fx:id="close" layoutX="820.0" layoutY="553.0" mnemonicParsing="false" onAction="#closeApplication" text="Close" />
      <Button fx:id="moveSongs" layoutX="450.0" layoutY="188.0" mnemonicParsing="false" onAction="#moveSongsToPlaylists" text="Move" />
      <Button fx:id="searchbutton" layoutX="828.0" layoutY="66.0" mnemonicParsing="false" onAction="#searchSong" text="Search" />
      <Button fx:id="moveUp" layoutX="239.0" layoutY="553.0" mnemonicParsing="false" onAction="#moveSongsUp" text="up" />
      <Button fx:id="moveDown" layoutX="285.0" layoutY="553.0" mnemonicParsing="false" onAction="#moveSongsDown" text="down" />
      <Label layoutX="240.0" layoutY="139.0" text="Songs in playlist" />
      <Button fx:id="editPlaylist" layoutX="74.0" layoutY="553.0" mnemonicParsing="false" onAction="#editExistingPlaylist" text="Edit" />
      <Button fx:id="editSong" layoutX="603.0" layoutY="553.0" mnemonicParsing="false" onAction="#editExistingSong" text="Edit" />
      <Label text="Label" />
      <TableView fx:id="songTable" layoutX="524.0" layoutY="168.0" prefHeight="376.0" prefWidth="347.0">
